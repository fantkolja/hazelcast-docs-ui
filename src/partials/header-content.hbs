<header class="header">
  <nav class="navbar">
    <div class="navbar-brand">
      <a class="navbar-item navbar-logo-item" href="/">
        <img src="{{{uiRootPath}}}/img/hazelcast-logo.svg" alt="Hazelcast Logo" />
        <p class="navbar-logo-item-title">Documentation</p>
      </a>
      <button class="navbar-burger" data-target="topbar-nav">
        <span></span>
        <span></span>
        <span></span>
      </button>
    </div>
    <div id="topbar-nav" class="navbar-menu">
      <div class="navbar-start">
        <div class="navbar-item has-dropdown is-hoverable">
          <a class="navbar-link" href="#" onclick="return false">Manuals</a>
          <div class="navbar-dropdown">
            <a class="navbar-item" href="{{{siteRootPath}}}/cloud/" onclick="window.open(this.href,'_blank');return false;">Hazelcast Cloud</a>
            <a class="navbar-item" href="{{{siteRootPath}}}/hazelcast/latest/" onclick="window.open(this.href,'_blank');return false;">Hazelcast Platform</a>
            <a class="navbar-item" href="{{{siteRootPath}}}/management-center/latest/" onclick="window.open(this.href,'_blank');return false;">Management Center</a>
            <a class="navbar-item" href="{{{siteRootPath}}}/operator/latest/" onclick="window.open(this.href,'_blank');return false;">Hazelcast Platform Kubernetes Operator</a>
            <a class="navbar-item" href="{{{siteRootPath}}}/clc/latest/" onclick="window.open(this.href,'_blank');return false;">Hazelcast Command-Line Client</a>
          </div>
        </div>
        {{#if (eq page.component.name 'hazelcast')}}
          <div class="navbar-item">
            <a class="navbar-item" href="{{{siteRootPath}}}/tutorials?product=platform">Tutorials</a>
          </div>
        {{else if (eq page.layout '404')}}
          <div class="navbar-item">
            <a class="navbar-item" href="{{{siteRootPath}}}/tutorials?product={{{this}}}">Tutorials</a>
          </div>
        {{else if (and (not-eq page.component.name 'tutorials')(not-eq page.component.name 'home'))}}
        {{#with page.component.name}}
          <div class="navbar-item">
            <a class="navbar-item" href="{{{siteRootPath}}}/tutorials?product={{{this}}}">Tutorials</a>
          </div>
        {{/with}}
        {{else}}
          <div class="navbar-item">
            <a class="navbar-item" href="{{{siteRootPath}}}/tutorials">Tutorials</a>
          </div>
        {{/if}}
        <div class="navbar-item has-dropdown is-hoverable">
          <a class="navbar-link" href="#" onclick="return false">API Reference</a>
          <div class="navbar-dropdown">
            <a class="navbar-item" {{~#if page.attributes.latest-supported-java-client}}href="https://docs.hazelcast.org/docs/{{page.attributes.latest-supported-java-client}}/javadoc"
            {{~else}}href="https://docs.hazelcast.org/docs/latest/javadoc"{{~/if}} onclick="window.open(this.href,'_blank');return false;">Java</a>
            <a class="navbar-item" {{~#if page.attributes.latest-supported-go-client}}href="https://pkg.go.dev/github.com/hazelcast/hazelcast-go-client@v{{page.attributes.latest-supported-go-client}}"
            {{~else}}href="https://pkg.go.dev/github.com/hazelcast/hazelcast-go-client?tab=versions"{{~/if}} onclick="window.open(this.href,'_blank');return false;">Go</a>
            <a class="navbar-item" {{~#if page.attributes.latest-supported-csharp-client}}href="http://hazelcast.github.io/hazelcast-csharp-client/{{page.attributes.latest-supported-csharp-client}}/api/index.html"
            {{~else}}href="https://hazelcast.github.io/hazelcast-csharp-client/versions.html"{{~/if}} onclick="window.open(this.href,'_blank');return false;">C#</a>
            <a class="navbar-item" {{~#if page.attributes.latest-supported-cplusplus-client}}href="http://hazelcast.github.io/hazelcast-cpp-client/{{page.attributes.latest-supported-cplusplus-client}}/index.html"
            {{~else}}href="http://hazelcast.github.io/hazelcast-cpp-client/api-index.html"{{~/if}} onclick="window.open(this.href,'_blank');return false;">C++</a>
            <a class="navbar-item" {{~#if page.attributes.latest-supported-python-client}} href="https://hazelcast.readthedocs.io/en/v{{page.attributes.latest-supported-python-client}}/index.html"
            {{~else}} href="https://readthedocs.org/projects/hazelcast/"{{~/if}} onclick="window.open(this.href,'_blank');return false;">Python</a>
            <a class="navbar-item" {{~#if page.attributes.latest-supported-nodejs-client}} href="http://hazelcast.github.io/hazelcast-nodejs-client/api/{{page.attributes.latest-supported-nodejs-client}}/docs/"
            {{~else}} href="http://hazelcast.github.io/hazelcast-nodejs-client/"{{~/if}} onclick="window.open(this.href,'_blank');return false;">Node.js</a>
          </div>
        </div>
        <div class="navbar-item has-dropdown is-hoverable">
          <a class="navbar-link" href="#" onclick="return false">Resources</a>
          <div class="navbar-dropdown">
            <a class="navbar-item" href="https://training.hazelcast.com/" onclick="window.open(this.href,'_blank');return false;">Online Training</a>
            <a class="navbar-item" href="https://hazelcast.com/hub/" onclick="window.open(this.href,'_blank');return false;">Integration Hub</a>
            <a class="navbar-item" href="https://hazelcast.com/use-cases/" onclick="window.open(this.href,'_blank');return false;">Use Cases</a>
            <a class="navbar-item" href="https://hazelcast.com/developers/" onclick="window.open(this.href,'_blank');return false;">Community</a>
            <a class="navbar-item" href="https://hazelcastcommunity.slack.com/archives/C012G4ETWNB"onclick=""window.open(this.href,'_blank');return false;">Community Slack</a>
            <a class="navbar-item" href="https://hazelcast.com/blog/" onclick="window.open(this.href,'_blank');return false;">Blog</a>
          </div>
        </div>
      </div>
      <div class="navbar-end">
        {{#if (and (not-eq page.layout 'home') (not-eq page.layout 'tutorials-landing'))}}
          <div id="search-input" class="navbar-item searchbox">
          </div>
          {{#if (not-eq page.layout '404')}}
            <label class="filter checkbox">
              <input id="facet" type="checkbox" onclick="applyFacetedSearch()"> Search all docs
            </label>
          {{/if}}
        {{/if}}
        {{#if (or (eq page.component.name 'cloud') (eq page.component.name 'tutorials'))}}
          <div class="navbar-item sign-in-btn">
            <a href="{{{page.attributes.cloud-console}}}" onclick="window.open(this.href,'_blank');return false;">
              Sign In
            </a>
          </div>
          <div class="navbar-item try-free-btn">
            <a href="{{{page.attributes.cloud-console}}}sign-up" onclick="window.open(this.href,'_blank');return false;">
              <button class="button">Try Free</button>
            </a>
          </div>
        {{else}}
          <div class="navbar-item"></div>
            <a class="get-hazelcast-link" href="https://hazelcast.com/get-started" onclick="window.open(this.href,'_blank');return false;">
              <button class="button">Get Hazelcast</button>
            </a>
          </div>
        {{/if}}
      </div>
    </div>
  </nav>
  {{> cookie}}
</header>
